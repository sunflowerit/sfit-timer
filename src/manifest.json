{
  "name": "SFIT-Timer",
  "version": "1.1",
  "description": "SFIT Timer cross-platform extension used to post work hours to Odoo timesheets",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/inactive_19.png",
      "38": "img/inactive_38.png"
    },
    "default_title": "SFIT Timer",
    "default_popup": "popup.html"
  },
  "devtools_page": "/devtools/devtools-page.html",
  "background": {
    "scripts": [
      "/js/lib/browser-polyfill.js",
      "/js/lib/jquery-2.0.3.min.js",
      "/js/background.js"
    ]
  },
  "options_ui": {
    "page": "options_main_page.html"
  },
  "permissions": [
    "tabs",
    "\u003Call_urls\u003E",
    "cookies",
    "contextMenus",
    "unlimitedStorage",
    "notifications",
    "storage",
    "clipboardWrite",
    "https://*/",
    "http://*/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "minimum_chrome_version": "41",
  "manifest_version": 2
}
